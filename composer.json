{
  "require": {
    "craftcms/cms": "3.1.29",
    "vlucas/phpdotenv": "^2.4.0",
    "craftcms/contact-form": "^2.2",
    "craftcms/ckeditor": "1.0.0-beta.2",
    "craftcms/store-hours": "2.1.1.1",
    "ether/simplemap": "3.4.11",
    "rias/craft-position-fieldtype": "1.0.14",
    "craftcms/google-cloud": "1.3.0",
    "codemonauts/craft-instagram-feed": "1.0.2",
    "aelvan/imager": "v2.1.10",
    "nystudio107/craft-minify": "1.2.9",
    "craftcms/redactor": "2.3.3.2",
    "verbb/field-manager": "2.1.0",
    "craftcms/contact-form-honeypot": "1.0.2",
    "kuriousagency/qrcode": "1.0.3"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "optimize-autoloader": true
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ]
  }
}
