{
  "require": {
    "craftcms/cms": "3.1.18",
    "vlucas/phpdotenv": "^2.4.0",
    "craftcms/contact-form": "^2.2",
    "craftcms/ckeditor": "1.0.0-beta.2",
    "fortrabbit/craft-copy": "1.0.0-RC4"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "optimize-autoloader": true
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ]
  }
}
